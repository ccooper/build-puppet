[db]
status_db = mysql://<%= scope.function_secret(["buildbot_statusdb_ro_username"])%>:<%= scope.function_secret(["buildbot_statusdb_ro_password"])%>@<%= scope.function_secret(["buildbot_statusdb_ro_hostname"])%>/buildbot
scheduler_db = mysql://<%= scope.function_secret(["buildbot_schedulerdb_ro_username"])%>:<%= scope.function_secret(["buildbot_schedulerdb_ro_password"])%>@<%= scope.function_secret(["buildbot_schedulerdb_ro_hostname"])%>/<%= scope.function_secret(["buildbot_schedulerdb_ro_database"])%>

[slavealloc]
api = http://slavealloc.build.mozilla.org/api

[repos]
buildbot_configs = http://hg.mozilla.org/build/buildbot-configs

[inventory]
api = https://releng-inventory-automation:<%= scope.function_secret(["slaveapi_inventory_password"])%>@inventory.mozilla.org

[graphite]
hosts = <%= scope.function_secret(["cruncher_graphite_hosts"])%>

[billing]
region = <%= scope.function_secret(["aws_billing_region"])%>
bucketname = <%= scope.function_secret(["aws_billing_bucketname"])%>
account = <%= scope.function_secret(["aws_billing_account"])%>
subaccount = <%= scope.function_secret(["aws_billing_subaccount"])%>
aws_accound_id = <%= scope.function_secret(["aws_billing_account_id"])%>
aws_secret_key = <%= scope.function_secret(["aws_billing_secret_key"])%>
